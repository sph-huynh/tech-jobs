<ul>
{% for post in postslist %}
  <li class="postlist-item{% if post.url == url %} postlist-item-active{% endif %}">
    <a href="{{ post.url | url }}" class="titled-links">
        {% if post.data.title %}{{ post.data.title }}{% else %}{{ post.data.career }}{% endif %}
    </a>
    <p class="descriptions">{{ post.data.outcomedesc }}</p>
  </li>
{% endfor %}
</ul>
